DOXYFILE_ENCODING      = UTF-8
PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@ \
                         @PROJECT_BINARY_DIR@
EXCLUDE                = @PROJECT_SOURCE_DIR@/json
INPUT                  = @doxy_main_page@ \
                         @PROJECT_SOURCE_DIR@ \
                         @PROJECT_BINARY_DIR@
FILE_PATTERNS          = *.h \
                         *.cc
RECURSIVE              = YES
USE_MDFILE_AS_MAINPAGE = @doxy_main_page@
GENERATE_HTML          = YES
GENERATE_XML           = YES
GENERATE_LATEX         = NO
GENERATE_RTF           = NO
